<h1>Registered Users</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Email</th>
      <th>Password</th>
      <th>Manage</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
     <td>
      <%= link_to admin_user_path(user), method: "delete", rel: "tooltip", title: "delete", data: { confirm: "Are you sure you want to delete this user?" } do %>
          <i class="icon-remove"></i>
          <% end %>
      </td> 
    </tr>
    <% end %>
  </tbody>
</table>
